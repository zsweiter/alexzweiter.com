{
    "compilerOptions": {
        "target": "ESNext",
        "moduleResolution": "bundler",
        "lib": ["ESNext", "DOM"],
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
        },
    },
}
